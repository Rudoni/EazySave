<Page x:Class="EazySave_Master.View.ViewMainWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EazySave_Master.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ViewMainWindow">

    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
        <Button Content="Create Save" HorizontalAlignment="Center" Margin="0,111,0,0" VerticalAlignment="Top" Height="78" Width="336" Click="Button_Click_CreateSave"/>
        <Button x:Name="RunSave" Content="Run Save" HorizontalAlignment="Center" Margin="0,240,0,0" VerticalAlignment="Top" Height="78" Width="336" Click="Button_Click_RunSave"/>

    </Grid>
</Page>
